    "### Extract SNP Data from a VCF File"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "\n",
    "vcf_file = \"input.vcf\"\n",
    "df = pd.read_csv(vcf_file, comment=\"#\", sep=\"\\t\", header=None)\n",
    "df.columns = [\"CHROM\", \"POS\", \"ID\", \"REF\", \"ALT\", \"QUAL\", \"FILTER\", \"INFO\"]\n",
    "snp_data = df[[\"CHROM\", \"POS\", \"REF\", \"ALT\"]]\n",
    "print(snp_data)\n"
   ]
  }
