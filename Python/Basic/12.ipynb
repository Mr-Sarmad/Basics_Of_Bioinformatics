   "source": [
    "import pandas as pd\n",
    "file = \"C:/Users/PC/Downloads/sequence_report.tsv\"\n",
    "threshold = 50  \n",
    "df = pd.read_csv(file, sep=\"\\t\")\n",
    "filtered = df[df['GC Percent'] > threshold] \n",
    "filtered.to_csv(\"filtered.tsv\", sep=\"\\t\", index=False)\n"
   ]
